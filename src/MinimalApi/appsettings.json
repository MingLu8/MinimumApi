{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "minimalApiMsSQL": "Server=(local);Database=minimalApi;user Id=sa;password=your-password;TrustServerCertificate=True;",
    "minimalApiSqlLite": "Data Source=./Data/minimalApi.db;Mode=ReadWrite;journal mode=Off;new=False;datetimeformat=CurrentCulture"
  },
  "ConsumerConfig": {
    "BootstrapServers": "localhost:22181,localhost:29092",
    "GroupId": "xxxxxxx",
    "AutoOffsetReset": "Earliest",
    "EnableAutoOffsetStore": false
  },
  "ProducerConfig": {
    "BootstrapServers": "localhost:9092,localhost:29092",
    "EnableDeliveryReports": true,
    "ClientId": "myproducer",
    "Debug": "msg",
    "Acks": "All",
    "MessageSendMaxRetries": 3,
    "RetryBackoffMs": 1000,
    "EnableIdempotence": true
  }
}
